{
    "name": "n8n-nodes-compo-pim-api",
    "version": "1.0.8",
    "description": "n8n community node for Compo PIM API integration",
    "keywords": ["n8n", "n8n-node", "compo", "pim", "api", "catalog", "product"],
    "author": "Andrej Spinej",
    "license": "MIT",
    "homepage": "https://github.com/Father1993/n8n-nodes-compo-pim-api",
    "repository": {
        "type": "git",
        "url": "https://github.com/Father1993/n8n-nodes-compo-pim-api"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc && npm run copy:icons",
        "copy:icons": "mkdir -p dist/nodes && cp -r nodes/icons dist/nodes/ 2>/dev/null || (if not exist dist\\nodes\\icons mkdir dist\\nodes\\icons && xcopy /s /y nodes\\icons\\*.svg dist\\nodes\\icons\\)",
        "dev": "tsc --watch",
        "format": "prettier --write .",
        "lint": "eslint . --ext .ts",
        "prepublishOnly": "npm run build"
    },
    "files": [
        "dist/**/*",
        "credentials/**/*",
        "nodes/**/*",
        "nodes/icons/*"
    ],
    "dependencies": {
        "axios": "^1.11.0"
    },
    "peerDependencies": {
        "n8n-core": "*",
        "n8n-workflow": "*"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "prettier": "^3.0.0",
        "typescript": "^5.9.2"
    },
    "engines": {
        "node": ">=18.10.0"
    },
    "n8n": {
        "credentials": [
            "dist/credentials/CompoPimApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/CompoPimApi.node.js"
        ]
    }
}
